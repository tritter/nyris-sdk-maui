<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>net9.0-android</TargetFramework>
        <SupportedOSPlatformVersion>21</SupportedOSPlatformVersion>
        <Nullable>enable</Nullable>
        <ImplicitUsings>enable</ImplicitUsings>
        <AssemblyName>Nyris.UI.Android.Camera</AssemblyName>
        <RootNamespace>Nyris.UI.Android.Camera</RootNamespace>
        <PackageId>Nyris.UI.Android.Camera</PackageId>
        <Authors>Nyris.UI.Android.Camera</Authors>
        <Company>Nyris.UI.Android.Camera</Company>
        <Product>Nyris.UI.Android.Camera</Product>
    </PropertyGroup>
    <ItemGroup>
      <AndroidLibrary Update="Jars\camera-lib-release.aar">
        <Bind>true</Bind>
        <Pack>true</Pack>
      </AndroidLibrary>
    </ItemGroup>
</Project>